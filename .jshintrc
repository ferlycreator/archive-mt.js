{
  "strict": false,
  "quotmark": "true", //只能使用单引号
  "noarg": true,
  "noempty": true, //不允许使用空语句块{}
  "eqeqeq": false, //!==和===检查
  "undef": true,
  "curly": true, //值为true时，不能省略循环和条件语句后的大括号
  "forin": true, //for in hasOwnPropery检查
  "devel": true,
  "browser": true,
  "evil": true,
  "sub":true,
  "unused": "vars", //形参和变量未使用检查
  "latedef": true, //先定义变量，后使用
  "globals": {
      "grunt": true,
      "module": true,
      "window": true,
      "global": true,
      "document": true,
      "console": true,
      "setTimeout": true,
      "setInterval": true,
      //mocha
      "describe" : true,
      "it" : true,
      "expect":true,
      //node.js
      "exports" : true,
      "module" : true,
      "maptalks": true,
      "Z": true
  }
}
